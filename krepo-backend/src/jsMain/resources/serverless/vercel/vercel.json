{
  "rewrites": [
    {
      "source": "/releases/:path*",
      "destination": "https://pkg.rtast.cn/releases/:path*"
    },
    {
      "source": "/snapshots/:path*",
      "destination": "https://pkg.rtast.cn/snapshots/:path*"
    },
    {
      "source": "/private/:path*",
      "destination": "https://pkg.rtast.cn/private/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://pkg.rtast.cn/api/:path*"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
